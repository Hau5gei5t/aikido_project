{
  "name": "aikido_project",
  "homepage": "https://Hau5gei5t.github.io/aikido_project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server:json": "json-server --watch db.json -m ./node_modules/json-server-auth -r routes.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:cv": "vitest run --coverage",
    "dev:full": "concurrently \"npm:dev\" \"npm:server:json\""
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "antd": "^5.11.2",
    "antd-mobile": "^5.33.0",
    "axios": "^1.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.19.0",
    "voucher-code-generator": "^1.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/voucher-code-generator": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "gh-pages": "^6.1.1",
    "jsdom": "^22.1.0",
    "json-server": "^0.17.4",
    "json-server-auth": "^2.1.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.6"
  }
}
